;(function($){$(document).ready(function($){$('pre').addClass('prettyprint')
    prettyPrint();var $subTitle=$('#top_nav .sub-menu li a');$subTitle.each(function(){$selfTitle=$(this);var $subText='<span>'+$selfTitle.attr('title')+'</span>';$selfTitle.append($subText)})
    $('#nav ul li:has(ul)').hover(function(){$(this).children('ul').stop(true,true).slideDown(400);},function(){$(this).children('ul').stop(true,true).slideUp('fast');})
    function lazyload(){$("img").each(function(){_self=$(this);if(_self.attr("data-original")&&(!_self.attr("src")||(_self.attr("src")&&_self.attr("data-original")!=_self.attr("src")))){if((_self.offset().top)<$(window).height()+$(document).scrollTop()+300){_self.attr("src",_self.attr("data-original"));}}});}
    lazyload();var itv;$(window).scroll(function(){clearTimeout(itv);itv=setTimeout(lazyload,100);});$(window).resize(function(){clearTimeout(itv);itv=setTimeout(lazyload,100);});share();function share(){var thelink=encodeURIComponent(document.location),thetitle=encodeURIComponent(document.title.substring(0,60)),windowName='分享到',param=getParamsOfShareWindow(600,560),s_tqq='http://v.t.qq.com/share/share.php?title='+thetitle+'&url='+thelink+'&site=',s_qzone='http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url='+thelink+'&title=',s_tsina='http://v.t.sina.com.cn/share/share.php?url='+thelink+'&title='+thetitle,s_douban='http://www.douban.com/recommend/?url='+thelink+'&title='+thetitle,s_renren='http://share.renren.com/share/buttonshare?link='+thelink+'&title='+thetitle,s_kaixin='http://www.kaixin001.com/repaste/share.php?rurl='+thelink+'&rcontent='+thelink+'&rtitle='+thetitle,s_facebook='http://facebook.com/share.php?u='+thelink+'&t='+thetitle,s_twitter='http://twitter.com/share?url='+thelink+'&text='+thetitle;$('.s-icon').each(function(){$(this).attr('title',windowName+$(this).text());$(this).click(function(){var httpUrl=eval($(this).attr('class').substring($(this).attr('class').lastIndexOf('s_')));window.open(httpUrl,windowName,param);});});function getParamsOfShareWindow(width,height){return['toolbar=0,status=0,resizable=1,width='+width+',height='+height+',left=',(screen.width-width)/2,',top=',(screen.height-height)/2].join('');}}});})(jQuery);